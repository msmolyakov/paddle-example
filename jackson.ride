{-# STDLIB_VERSION 3 #-}
{-# CONTENT_TYPE DAPP #-}
{-# SCRIPT_TYPE ACCOUNT #-}

@Callable(inv)
func some(bin: ByteVector, bool: Boolean, int: Int, str: String) = {
    ScriptResult(
        WriteSet([
            DataEntry("bin", bin),
            DataEntry("bool", bool),
            DataEntry("int", int),
            DataEntry("str", str)
        ]),
        TransferSet([
            ScriptTransfer(inv.caller, 10000000, unit)
        ])
    )
}
